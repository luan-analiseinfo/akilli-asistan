<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.mehmetzahit.entities.TopicContent" table="content">
		<id name="contentID" column="contentID">
			<generator class="native"></generator>
		</id>
		
		<property name="contentTitle"></property>
		<property name="content"></property>
		<property name="keywords"></property>
		<property name="rating"></property>
		<property name="memberID"></property>
		<property name="topicID"></property>
	
		

	</class>

</hibernate-mapping>
