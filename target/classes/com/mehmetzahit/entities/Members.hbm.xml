<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.mehmetzahit.entities.Members" table="members">
		<id name="memberID" column="memberID">
			<generator class="native"></generator>
		</id>
		
		<property name="name" column="Name"></property>
		<property name="surname" column="Surname"></property>
		<property name="email" column="Email"></property>
		<property name="password" column="Password"></property>
		
<!-- 		<many-to-one name="topic"  class="com.mehmetzahit.entities.Topics" fetch="select" cascade="save-update" /> -->
		

	</class>

</hibernate-mapping>

