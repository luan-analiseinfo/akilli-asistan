<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.mehmetzahit.entities.TopicCodeContent" table="topics">
		<id name="codeID" column="codeID">
			<generator class="native"></generator>
		</id>
		
		<property name="codeTitle" column="codeTitle"></property>
		<property name="codeContent" column="codeContent"></property>		
		<property name="contentID" column="contentID"></property>
		<property name="topicID" column="topicID"></property>
		<property name="memberID" column="memberID"></property>
		
	

	</class>

</hibernate-mapping>

