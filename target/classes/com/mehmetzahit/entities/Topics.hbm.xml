<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.mehmetzahit.entities.Topics" table="topics">
		<id name="topicID" column="topicID">
			<generator class="native"></generator>
		</id>
		
		<property name="topicName" column="TopicName"></property>
		<property name="memberID" column="memberID"></property>
		
<!-- 		<many-to-one name="members"  class="com.mehmetzahit.entities.Members" fetch="select"  /> -->
	</class>

</hibernate-mapping>